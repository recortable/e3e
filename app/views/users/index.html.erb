
<table>
  <thead>
    <tr>
      <td>Nombre de usuario</td>
      <td>Última vez que entró</td>
      <td></td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr class="<%= cycle('even', 'odd') %>">
        <td><%= user.username %></td>
        <td><%= user.last_login_at ? user.last_login_at.fecha : 'todavía no ha entrado' %> </td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>